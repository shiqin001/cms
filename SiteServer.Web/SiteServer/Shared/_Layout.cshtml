@{ var adminUrl = SiteServer.CMS.Env.AdminUrl; var apiUrl = SiteServer.CMS.Env.ApiUrl; }
<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8" />
  <title>SiteServer 管理后台</title>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="renderer" content="webkit" />
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <link href="@adminUrl/assets/images/favicon.png" rel="icon" type="image/png">
  <link href="@adminUrl/assets/css/ionicons-2.0.0.min.css" rel="stylesheet" type="text/css" />
  <link href="@adminUrl/assets/css/font-awesome-4.7.0.min.css" rel="stylesheet" type="text/css" />
  <link href="@adminUrl/assets/css/bootstrap-4.1.0.min.css" rel="stylesheet" type="text/css" />
  <link href="@adminUrl/assets/css/siteserver.min.css" rel="stylesheet" type="text/css" /> @RenderSection("Styles",
  false)
</head>

<body>
  <div id="main" class="m-3">
    <template v-if="pageLoad"> @RenderBody() </template>
    <template v-else>
      <div class="text-center" style="margin-top: 100px">
        <img class="mt-3" src="@adminUrl/assets/images/loading.gif" />
        <p class="lead mt-3 text-nowrap">载入中，请稍后...</p>
      </div>
    </template>
  </div>
</body>

</html>
<script type="text/javascript">
  var $apiUrl = '@apiUrl';
</script>
<script src="@adminUrl/assets/lib/es6-promise.auto.min.js" type="text/javascript"></script>
<script src="@adminUrl/assets/lib/axios-0.17.1.min.js" type="text/javascript"></script>
<script src="@adminUrl/assets/lib/jquery-1.9.1.min.js" type="text/javascript"></script>
<script src="@adminUrl/assets/lib/lodash-4.17.10.min.js" type="text/javascript"></script>
<script src="@adminUrl/assets/lib/layer-3.1.1/layer.js" type="text/javascript"></script>
<script src="@adminUrl/assets/lib/sweetalert2-7.28.4.all.min.js" type="text/javascript"></script>
<script src="@adminUrl/assets/lib/vue-2.5.16.min.js" type="text/javascript"></script>
<script src="@adminUrl/assets/lib/vee-validate-2.1.0.js" type="text/javascript"></script>
<script src="@adminUrl/assets/lib/vee-validate-locale-zh_CN-2.1.0.js" type="text/javascript"></script>
<script src="@adminUrl/assets/js/utils.js" type="text/javascript"></script> @RenderSection("Scripts", false)