@{ Layout = "../Shared/_Layout.cshtml"; } @section Styles { <style>html, body{background-color: #fff;}</style> }
<template v-if="pageAlert">
  <div class="alert" :class="{ 'alert-warning': pageAlert.type === 'warning', 'alert-success': pageAlert.type === 'success', 'alert-danger': pageAlert.type === 'danger' }">
    <button v-on:click="pageAlert = null" class="close" data-dismiss="alert">×</button>
    <span v-html="pageAlert.html"></span>
  </div>
</template>
<div class="form-horizontal">
  <div class="form-group row">
    <label class="col-3 text-right col-form-label">名称</label>
    <p class="col-5 form-control-plaintext"> {{ tokenInfo && tokenInfo.title }} </p>
    <div class="col-4 help-block"></div>
  </div>
  <div class="form-group row">
    <label class="col-3 text-right col-form-label">API密钥</label>
    <div class="col-5 form-control-plaintext">
      <code style="font-size: 16px"> {{ accessToken }} </code>
    </div>
    <div class="col-4 help-block">
      <button v-on:click="btnRegenerateClick" class="btn btn-success m-l-5">重 设</button>
    </div>
  </div>
  <div class="form-group row">
    <label class="col-3 text-right col-form-label">添加时间</label>
    <p class="col-5 form-control-plaintext"> {{ tokenInfo && tokenInfo.addDate }} </p>
    <div class="col-4 help-block"></div>
  </div>
  <div class="form-group row">
    <label class="col-3 text-right col-form-label">更新时间</label>
    <p class="col-5 form-control-plaintext"> {{ tokenInfo && tokenInfo.lastModifiedDate }} </p>
    <div class="col-4 help-block"></div>
  </div>
</div>
<hr />
<div class="text-right mr-1">
  <button type="button" v-on:click="btnCancelClick" class="btn btn-default m-l-5">关 闭</button>
</div> @section Scripts{ <script src="adminAccessTokensViewLayer.js" type="text/javascript"></script> }
